}

uChartView{
    background-color:rgba(22, 119,255,255);
    border: 1px solid white;   /* 边框 */
}

/*=========================== QCheckBox Chart============================*/
QWidget#ParameterConfig{
    background-color:rgba(22,25,41,255);
    border: 0px solid gray;   /* 边框 */
}


QListView#ParameterConfig{
    background-color:rgba(6,7,19,255);
    border: 1px solid rgba(120,120,120,120);   /* 边框 */
    color:#FFFFFF;
    font: 450 9pt "Microsoft YaHei";

    min-width:100px;
}


QListView#ParameterConfig::item{
    color:#FFFFFF;
    font: 450 9pt "Microsoft YaHei";
    text-align:center;
    width:100;
    height:30;
    margin-top:1px;
    margin-right:3px;
    margin-left:3px;
    margin-bottom:1px;
    min-height:35px;
    background-color:rgba(22, 119,255,0.2);
}


QListView#ParameterConfig::item:hover{
    color:#FFFFff;
    font: 450 9pt "Microsoft YaHei";
    background-color:rgba(22, 119,255,200);
    /* background-color:rgba(14, 174, 105,255);*/
}

QListView#ParameterConfig::item:pressed{
    color:#FFFFff;
    font: 450 9pt "Microsoft YaHei";
    background-color:rgba(22, 119, 255,120);
}

QListView#ParameterConfig::item:selected{
    color:#FFFFff;
    font: 450 9pt "Microsoft YaHei";
    background-color:rgba(22, 119, 255,255);
}


QSplitter#ParameterConfig::handle {
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,stop:0 rgba(61,61,61,255),stop:0.1 rgba(61,61,61,255),stop:0.2 rgba(22,25,41,255));
}

QSplitter#ParameterConfig::handle:pressed {
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,stop:0 rgba(122,122,122,255),stop:0.5 rgba(122,122,122,255),stop:1 rgba(22,25,41,255));
}

QStackedWidget#ParameterConfig{
    background-color:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0,stop:0 rgba(20,20,20,0.7),stop:0.3 rgba(20,20,20,0.8),stop:0.7 rgba(20,20,20,0.8),stop:1 rgba(20,20,20,0.7));
    border-right: 5px solid qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,stop:0 rgba(20,20,20,0),stop:0.5 rgba(20,20,20,20==0.9),stop:1 rgba(20,20,20,0)) ;
}


