}

/*checkbox样式设置*/
QCheckBox::indicator {
width: 17px;
height: 17px;
}
/*未选中*/
QCheckBox::indicator::unchecked {
    image: url(:/img/checkstate/unselect.png);
}

/*选中*/

QCheckBox::indicator::checked {
    image: url(:/img/checkstate/select.png);
}

