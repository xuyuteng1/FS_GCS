color: white;
max-width:20px;
font: 400 8.5pt "Microsoft YaHei";
/* background-color:rgb(101,101,101); */
}

/*checkbox样式设置*/
QCheckBox#check_chart::indicator {
width: 17px;
height: 17px;
}
/*未选中*/
QCheckBox#check_chart::indicator::unchecked {
    image: url(:/img/checkstate/unselect.png);
}

/*选中*/

QCheckBox#check_chart::indicator::checked {
    image: url(:/img/checkstate/select.png);
}



/*RadioButton样式设置*/
QRadioButton::indicator {
        width: 17px;
        height: 17px;
}

/*单选框未选中样式*/
QRadioButton::indicator::unchecked {
    image: url(:/img/checkstate/unselect.png);
}

/*单选框选中样式*/
QRadioButton::indicator::checked {
        image: url(:/img/checkstate/select.png);
}

/*RadioButton和checkbox字体和间距设置*/
QRadioButton ,QCheckBox{
        spacing: 5px;
        font-size: 15px;
}

