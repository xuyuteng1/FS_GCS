QMenu#motorUnlockMenu,QMenu#modelMenu,QMenu#touchDownMenu{
    background-color: rgba(80,80,80,255);
    font: 400 10pt "Microsoft YaHei UI";
    color:rgb(200,200,200);
    border-radius:2px;
    width:110pt;
}

QMenu#motorUnlockMenu::item,QMenu#modelMenu::item,QMenu#touchDownMenu::item{
    width:100;
    height:30px;
    background-color:rgb(80,80,80);
    border:1px solid rgb(55,55,55);
    border-radius:1px;
    padding: 0px 0px 0px 20px;
    margin:1px 1px 1px 1px;
}
QMenu#motorUnlockMenu::item:selected,QMenu#modelMenu::item:selected,QMenu#touchDownMenu::item:selected{
    background-color:rgb(40,40,40);
}


