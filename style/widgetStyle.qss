/* ********************************************************************************************************************
// 主界面标题栏样式
*******************************************************************************************************************   */

QWidget#widgetitle,QWidget#TitleWidget{
    background-color:rgba(6,7,19,255);
    border: 0pt solid gray;   /* 边框 */
}

QLabel#titleLabel{
    color:#FFFFFF;
    font: 400 11pt "Microsoft YaHei UI";
    background-color:rgba(22,25,41,0);
}


QLabel#locationLng,QLabel#locationLat,QLabel#dateTimeLabel{
    color:#FFFFFF;
    font: 400 9pt "Microsoft YaHei UI";
    background-color:rgba(22,25,41,0);
}



QPushButton#closeButton,QPushButton#connectionButton,QPushButton#minimizeButton,QToolButton#selectButton{
    background-color:rgba(9,9,95,0);
    font: 400 11pt "Microsoft YaHei UI";
    color:#FFFFFF;
    border-radius:5px;
}

QPushButton#closeButton::hover
{
    background-color:rgba(255,9,9,0.4);
}
QPushButton#minimizeButton::hover,QPushButton#QToolButton::hover{
    background-color:rgba(9,255,255,0.4);
}

QPushButton#closeButton::pressed
{
    background-color:rgba(255,9,9,0.8);
}

QPushButton#minimizeButton::pressed,QPushButton#QToolButton::pressed{
    background-color:rgba(9,255,255,0.6);

}


QPushButton#connectionButton:hover{

    font: 400 11pt "Microsoft YaHei UI";
    color:#6FFF6F;
    background-color:rgba(9,255,200,0.2);
}

QPushButton#connectionButton:pressed{
    font: 400 11pt "Microsoft YaHei UI";
    color:#2FFF2F;
    background-color:rgba(9,255,200,0.6);
}


/* ********************************************************************************************************************
// 消息弹窗提示界面
*******************************************************************************************************************   */
QListView#ChatWidget{
    background-color:rgba(6,7,19,0.95);
